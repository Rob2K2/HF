		<LeftNavButton id="ctrlSearch" onClick="search" platform="ios" systemButton="Ti.UI.iPhone.SystemButton.SEARCH" />
		<TableView id="tableHotels" onClick="doClickTable">
			<TableViewSection></TableViewSection>
		</TableView>
		<Toolbar platform="ios" id='menuToolBar' >
